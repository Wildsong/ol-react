{
  "name": "@map46/ol-react",
  "version": "0.3.0",
  "description": "React interface for OpenLayers 5",
  "keywords": [
    "mapping",
    "gis",
    "openlayers",
    "ol",
    "ol5",
    "react"
  ],
  "main": "./index.html",
  "scripts": {
    "test": "parcel serve test.html --open",
    "start": "parcel serve index.html --open",
