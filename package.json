  },
  "homepage": "https://github.com/brian32768/ol-react#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-object-assign": "^7.0.0",
