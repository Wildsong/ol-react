    "cssnano": "^4.1.10",
    "eslint": "^6.1.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "mapbox-gl-styles": "^2.0.2",
    "ol": "https://github.com/openlayers/openlayers/archive/v6.0.0-beta.11.tar.gz"
  },
  "dependencies": {
    "@geonet/geohash": "^1.0.10",
    "@mapbox/mapbox-gl-style-spec": "^13.7.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "cors": "^2.8.5",
    "cuid": "^2.1.6",
    "jsonp": "^0.2.1",
    "mapbox-to-css-font": "^2.4.0",
    "ol-ext": "^3.1.4",
